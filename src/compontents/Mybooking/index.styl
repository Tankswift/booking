#Mybooking{
    width 100vw
    height 100vh
    position fixed
    z-index: 100
    background: rgba(0,0,0,0.56)
    top 0
    left 0  
    bottom 0
    right 0 
    .Box{
        height 91.3vw
        position: absolute
        bottom 0
        width: 100vw
        padding-top: 4.7vw
        background:rgba(255,255,255,1);
        border-radius:2.1vw 2.1vw 0 0;
        .title{
            font-size:4vw;
            font-family:Microsoft YaHei;
            font-weight:bold;
            color:rgba(51,51,51,1);
            line-height:4.3vw;
            text-align: center
            position relative    
            z-index 999
        }
        .border{
            width 16.3vw
            height 1.6vw
            background rgba(255,198,56,1)
            position absolute
            left: 0
            right 0
            margin 0 auto 
            top:8vw
        }
        .remind{
            margin-top:4vw
            width:100vw
            height: 10.1vw
            background:rgba(255,227,222,1)
            font-size:3.2vw
            font-family:Microsoft YaHei
            font-weight:400
            color:rgba(234,79,50,1)
            line-height:10.1vw
            text-align: center
        }
        .bookingContent{
            height 73vw
            padding: 0 4.4vw 0 5.3vw
            font-size:2.9vw;
            font-family:Microsoft YaHei;
            font-weight:400;
            overflow-y:scroll;
            color:rgba(153,153,153,1);
            .bookingContentItem{
                border-bottom:0.3vw solid rgba(238,238,238,1);
                padding-bottom: 3.5vw
                margin-top: 4vw
                
            .head{
                overflow: hidden
                white-space: nowrap
                text-overflow: ellipsis
                font-size:3.2vw;
                font-family:Microsoft YaHei;
                font-weight:bold;
                color:rgba(51,51,51,1);
                line-height:4.8vw;
            }
            .time{
                display flex
                margin-top: 2.8vw
                align-items: center
                img{
                    width 2.5vw
                    height 2.5vw
                    margin-right:2.3vw
                }
                
            }
            .place{
                align-items: center
                display flex
                margin-top: 2vw
                img{
                    width 2.3vw
                    height 2.4vw
                    margin-right:2.3vw
                    
                }
                
                }
            }    
       }
       .empty{
                img {
                    width 24.3vw
                    height 22.7vw
                    display block
                    margin:12.8vw 37.9vw 0 37.7vw
                }
                .emptyTxt{
                    font-size:3.2vw
                    font-family:Microsoft YaHei
                    font-weight:400
                    color:rgba(153,153,153,1)
                    line-height:4.3vw
                    margin-top: 0.8vw
                    text-align: center
                }
                
            }
        .close{
            position absolute
            right: 2.9vw
            top:3vw
            img {
            width 3.6vw
            height 3.6vw
            display block
        }
           
    }
    }
    
}